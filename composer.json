{
	"name": "pd/facebook-login-api",
	"description": "Facebook Login API",
	"keywords": ["facebook", "login", "api"],
	"type": "library",
	"homepage": "https://github.com/peckadesign/FacebookLoginApi",
	"authors": [
		{
			"name": "Marek Hučík"
		}
	],
	"require": {
		"php": "^7.4 | ^8.0",
		"nickdnk/graph-sdk": "^6.0",
		"nette/http": "^3.0",
		"nette/application": "^3.0",
		"nette/di": "^3.0"
	},
	"require-dev": {
		"pd/coding-standard": "^1.27",
		"phpstan/phpstan": "^1.4.6"
	},
	"autoload": {
		"psr-4": {
			"Pd\\FacebookLoginApi\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
